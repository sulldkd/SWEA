#define _CRT_SECURE_NO_WARNINGS
#include <iostream>
#include <algorithm>
#include <vector>
#include <cstring>
using namespace std;


int dx[8] = {-1,-1,-1,0,1,1,1,0};
int dy[8] = {-1,0,1,1,1,0,-1,-1};
int check;
int arr[9][9] = { 0 };
int sum = 0;
void Check(int x,int y) {
	sum = sum + arr[x][y];
	for (int i = 0; i < 8; i++) {
		int nx = x + dx[i];
		int ny = y + dy[i];
		sum = sum + arr[nx][ny];
	}
	if (sum != 45) {
		check++;
	}
	sum = 0;
}
int main(void) {

	ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);

	int t;
	cin >> t;

	for (int tc = 1; tc <= t; tc++) {
		
		
		for (int i = 0; i < 9; i++) {
			for (int j = 0; j < 9; j++) {
				cin >> arr[i][j];
				sum = sum + arr[i][j];
			}
			if (sum != 45) {
				check++;
			}
			sum = 0;
		}
	

		for (int i = 0; i < 9; i++) {
			for (int j = 0; j < 9; j++) {
				sum = sum + arr[j][i];
			}
			if (sum != 45) {
				check++;
			}
			sum = 0;
		}
		for (int i = 1; i <= 7; i = i + 3) {
			for (int j = 1; j <= 7; j = j + 3) {
				
				Check(i, j);
			}
		}

		if (check == 0) {
			cout << "#" << tc << " 1" << endl;
		}
		else {
			cout << "#" << tc << " 0" << endl;
		}
		check = 0;
		memset(arr, 0, sizeof(arr));

	}
	return 0;
}


